{
  "name": "Recietas",
  "version": "1.0.0",
  "description": "David's Recipe App",
  "repository": "https://github.com/dev-academy-challenges/taxonomic-routing.git",
  "main": "server/index.js",
  "scripts": {
    "start": "run-p start:*",
    "start:server": "node server",
    "start:client": "webpack",
    "dev": "run-p dev:*",
    "dev:client": "webpack --watch",
    "dev:server": "node server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db-reset": "run-s db:*",
    "db:delete": "rm -f server/db/dev.sqlite3",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "postinstall": "run-s db:migrate db:seed",
    "test": "jest --watchAll"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "jest": {
    "testURL": "http://localhost/",
    "setupFiles": [
      "./tests/setup-env.js"
    ]
  },
  "author": "EDA",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "express": "^4.16.4",
    "formik": "^1.5.7",
    "knex": "^0.19.5",
    "react-virtualized-select": "^3.1.3",
    "sqlite3": "^4.0.9",
    "yup": "^0.27.0"
  }
}
